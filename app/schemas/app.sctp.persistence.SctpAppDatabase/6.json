    "views": [
      {
        "viewName": "SessionView",
        "createSql": "CREATE VIEW `${VIEW_NAME}` AS select p.*, (select coalesce(count(householdId), 0) from households where sessionId = p.id and selection = 'Selected') selectedHouseholds from pev_sessions p"
